body {
    margin: 0;
}
/* <!-- button block -->*/
.but {
    font-family: Arial;
    width: 100%;
    height: 50px;
    color: white;
    font-weight: bolder;
    background-color: #0275d8;
    border: 0;
    font-size: 15px;
}
/* <!-- navbar -->*/
.navbar {
    margin-bottom: 0;
}

.navbar-header a, .navbar-right li a {
    color: #563d7c !important;
    font-family: Arial;
    font-size: 16px;
}
.nav > li > a {
    font-weight: 500;
    color: #563d7c !important;
    font-size:14px;
}
 .container-fluid>.navbar-collapse, 
 .container-fluid>.navbar-header, 
 .container>.navbar-collapse,
 .container>.navbar-header {
    margin-right: 0;
    margin-left: 35px;
}
.navbar-brand {
    float: left;
    height: 50px;
    padding: 15px 15px;
    font-size: 18px;
    line-height: 20px;
}
.navbar-header a:first-child {
    font-size: 16px;
}
/* <!-- section one about bootstrap -->*/
.b {
    text-align: center;
    height: 700px;
    width: auto;
    background-color: #6f5499;
    padding: 5%;
}

.B {
    border: 1px solid white;
    width: 150px;
    height: 150px;
    margin: auto;
    border-radius: 30px;
}

    .b p {
        font-family: Arial;
        font-size: 30px;
        color: white;
        width: 90%;
        margin: auto;
        padding: 3%;
    }

    .B span {
        position: relative;
        bottom: 3%;
        font-family: Helvetica Neue, Arial, sans-serif;
        font-size: 120px;
        color: white;
    }

    .b button {
        background: none;
        background: #6f5499;
        border: 1px solid white;
        height: 57px;
        width: 230px;
        color: white;
        font-family: Arial;
        font-size: 19px;
        border-radius: 5px;
        margin-bottom: 2%;
    }

 
    .b label {
        color: white;
        opacity: 0.3;
        font-family: MS UI Gothic;
        font-size: 14px;
        margin-bottom: 2%;
    }

label {
    display: inline-block;
    max-width: 100%;
    margin-bottom: 5px;
    font-weight: normal;
}

    .b .cover {
        display: flex;
        border: 1px solid #866ab3;
        width: 30%;
        height: 150px;
        margin: auto;
    }

    .b .img1 img {
        padding-top: 10px;
        padding-left: 10px;
        height: 140px;
        width: 140px;
    }

    .b .p1 p {
        font-family: Arial;
        font-size: 17px;
        color: white;
        margin: 0;
        width: 70%;
        padding-top: 10%;
    }

    .b .p1 span {
        font-family: Arial;
        font-size: 12px;
        color: gainsboro;
        margin: 0;
        position: relative;
        right: 20%;
    }
/*<!-- section two about header -->*/

.header .title h1 {
    text-align: center;
    font-family: Arial;
    font-size: 40px;
    padding-top: 9%;
}

.header .title p {
    text-align: center;
    font-family: Arial;
    font-size: 22px;
    color: #555555;
    width: 85%;
    margin: auto;
}

.header hr ,.center hr{
    width: 10%;
    margin-top: 30px;
}

.header .size img {
    padding-top: 20px;
    width: 370px;
    height: 130px;
}

.header .size div {
    text-align: center;
    font-family: Arial;
    font-size: 15.6px;
    margin-bottom: 40px;
}

.header .view {
    text-align: center;
    font-family: Arial;
}

    .header .view p {
        font-size: 25px;
        margin: 2%;
    }

    .header .view button {
        font-size: 15px;
        color: #563d7c;
        background-color: transparent;
        border: 0;
        border: 1px solid #563d7c;
        border-radius: 10px;
        width: 250px;
        height: 50px;
        margin-bottom: 5%;
    }
/*  <!-- section about  Bootstrap theames -->*/
.center {
    text-align: center;
    font-family: Arial;
    font-size: 15px;
    margin-top: 7%;
}

    .center p {
        width: 80%;
        margin: auto;
        font-size: 20px;
        margin-bottom: 15px;
    }

    .center button {
        font-size: 17px;
        color: #563d7c;
        background-color: transparent;
        border: 1px solid #563d7c;
        border-radius: 10px;
        width: 160px;
        height: 50px;
        margin-bottom: 2%;
    }
.center img
{
    width:90%;
    margin-bottom:8%;
}
.changeImg img
{
    width:350px;
    margin-bottom:0;
}
/*  <!-- section about footer -->*/
.footer
{
    background: black;
    width: 100%;
    height: 190px;

}
    .footer ul
    {
        margin:0;
    }
    .footer ul li {
        display: inline-block;
        text-decoration: none;
        padding-top: 50px;
        padding-right: 15px;
        padding-bottom: 20px;

    }
    .footer ul li a
    {
        color:white;
        font-family:Arial;
        position:relative;
        right:20px;
       
        
    }
.footer div p
{
    color:#99979c;
    margin:0;
    padding-left:17px;
}
    .footer div p span
    {
        color:white;
    }
/*   <!-- section one about headerbody -->*/
.Getting {
    width: 100%;
    height: 350px;
    background: #6b5094;
}
.GettingStart .pragraph
{
    font-family:Arial;
    color:white;
}
.GettingStart .pragraph h1
{
    font-size:65px;
    padding-top:8%;
}
    .GettingStart .pragraph p {
        font-size: 25px;
        color: #cdbfe3;
    }
.all .cover {
    display: flex;
    border: 1px solid #866ab3;
    width: 100%;
    height: 132px;
    margin-top:25%;
    margin-right:25%;
    
}
    .all .cover .img1 img {
        padding-top: 10px;
        padding-left: 10px;
        height: 120px;
        width: 160px;
    }
    .all .cover .p1 p {
        font-family: Arial;
        font-size: 15px;
        color: white;
        padding-top: 10%;
        padding-left: 5%;
        margin:0;
    }
    .all .cover .p1 span {
        font-family: Arial;
        font-size: 12px;
        color: gainsboro;
        margin: 0;
        padding-left: 5%;
    }